{
    "version": "1.6.2",
    "compilerOptions": {
        "target": "es5",
        "module": "amd",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "outDir": "dist",
        "sourceMap": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./test/app.spec.ts",
        "./test/converters.spec.ts"
    ],
    "exclude": [],
    "atom": {
        "rewriteTsconfig": true
    }
}
